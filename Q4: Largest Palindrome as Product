# Find the largest palindrome that is the product of two three-digit numbers.

def isPalindrome(num):
    if ((num//100000 == num%10) 
    and ((num//10)%10 == (num//10000)%10)
    and ((num//100)%10 == (num//1000)%10)):
        return True
    return False

def largePalindrome():
    large = 0
    for i in range(900,1000):
        for k in range(900,1000):
            if ((isPalindrome(i*k) == True) and (i*k > large)):
                large= i*k
    return large

largePalindrome()
